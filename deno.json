{
  "name": "@mage/tailwindcss",
  "version": "0.2.3",
  "license": "MIT",
  "exports": {
    ".": "./plugin/mod.ts"
  },
  "tasks": {
    "example-build": "deno run -A ./example/build.ts",
    "example-develop": "deno run -A --watch-hmr ./example/develop.ts",
    "example-serve": "deno run -A ./example/serve.ts",
    "test": "deno test -A --clean --coverage",
    "coverage": "deno coverage",
    "lint": "deno lint",
    "format": "deno fmt",
    "check": "deno check ."
  },
  "imports": {
    "@mage/app": "jsr:@mage/app@^0.6.1",
    "@std/assert": "jsr:@std/assert@^1.0.11",
    "@std/async": "jsr:@std/async@^1.0.10",
    "@std/expect": "jsr:@std/expect@^1.0.13",
    "@std/fs": "jsr:@std/fs@^1.0.11",
    "@std/path": "jsr:@std/path@^1.0.8",
    "@std/testing": "jsr:@std/testing@^1.0.9",
    "@tailwindcss/cli": "npm:@tailwindcss/cli@^4.0.8"
  },
  "nodeModulesDir": "auto"
}
